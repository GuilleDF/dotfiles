module(_,_).

nat(0).
nat(s(X)) :- nat(X).

plus(X,0,X) :- nat(X).
plus(X,s(Y),s(Z)) :- plus(X,Y,Z).

times(X,0,0) :- nat(X).
times(X,s(Y),Z) :- times(X,Y,T), plus(X,T,Z).

exp(X,0,s(0)) :- nat(X).
exp(X,s(Y),Z) :- exp(X,Y,T), times(X,T,Z).

ackermann(0,N,s(N)).
ackermann(s(M),0,A) :- ackermann(M,s(0),A).
ackermann(s(M),s(N),A) :- ackermann(M,Tmp,A), ackermann(s(M),N,Tmp).

myappend([],Ys,Ys).
myappend([X|Xs],Ys,[X|Zs]) :- myappend(Xs,Ys,Zs).

mymember(X,[X|_]).
mymember(X,[_|Ys]) :- mymember(X,Ys).

prefix([],_).
prefix([X|Xs],[X|Ys]) :- prefix(Xs,Ys).

suffix(Xs,Xs).
suffix(Xs,[_|Ys]) :- suffix(Xs,Ys).

sublist_r(Xs,Ys) :- prefix(Xs,Ys).
sublist_r(Xs,[_|Ys]) :- sublist(Xs,Ys).

sublist(Xs,Ys) :- prefix(X,Ys), suffix(Xs,X).

reverse_acc(Xs,Ys) :- reverse_acc(Xs,[],Ys).

reverse_acc([],Ys,Ys).
reverse_acc([X|Xs],Acc,Ys) :- reverse_acc(Xs,[X|Acc],Ys).