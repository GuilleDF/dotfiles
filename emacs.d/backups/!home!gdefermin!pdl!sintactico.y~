%{
  #include "yylex.h"
%}

%define api.value.type {intptr_t}

%token INT 16
%token STR 1
%token ADD 2
%token GT 3
%token NEG 4
%token DEC 5
%token ASG 6
%token OPP 7
%token CLP 8
%token OPB 9
%token CLB 10
%token END 11
%token COM 12
%token ID 14
%token PR 15
%token UNDEFINED 20
%token ERR 17

%start p

%%

p:
   | b z p
   | z p
   ;

z:   END
   ;
b:   PR t ID 
   ;
t:   PR
   ;

%%
